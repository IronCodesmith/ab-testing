@model EPiServer.Marketing.Multivariate.MultivariateTest

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>GetAbTest</title>
</head>
<body>
    <fieldset>
        <legend>IMultivariateTest</legend>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.Title)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Title)
        </div>
        <div class="display-label">
            @Html.DisplayNameFor(model => model.State)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.State)
        </div>
        <div class="display-label">
            @Html.DisplayNameFor(model => model.Owner)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.Owner)
        </div>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.StartDate)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.StartDate)
        </div>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.EndDate)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.EndDate)
        </div>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.OriginalItemId)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.OriginalItemId)
        </div>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.VariantItemId)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.VariantItemId)
        </div>

        <div class="display-label">
            @Html.DisplayNameFor(model => model.ConversionItemId)
        </div>
        <div class="display-field">
            @Html.DisplayFor(model => model.ConversionItemId)
        </div>

        @foreach (var result in Model.Results)
        {
            <div class="display-label">
                @Html.DisplayNameFor(resultItem => result.Views)
            </div>
            <div class="display-field">
                @Html.DisplayFor(resultItem => result.Views)
            </div>
            <div class="display-label">
                @Html.DisplayNameFor(resultItem => result.ItemId)
            </div>
            <div class="display-field">
                @Html.DisplayFor(resultItem => result.ItemId)
            </div>
            <div class="display-label">
                @Html.DisplayNameFor(resultItem => result.Conversions)
            </div>
            <div class="display-field">
                @Html.DisplayFor(resultItem => result.Conversions)
            </div>
        }

    </fieldset>

</body>
</html>
